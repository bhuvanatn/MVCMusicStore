@model MVCMusicStore.ViewModel.StoreIndexViewModel

@{
    ViewBag.Title = "Browse Genre";
}

<h2>Browse Genre</h2>

<p>Select from @Model.NumberOfGenres genres</p>

@foreach (string genreName in Model.Genres)
{
<li>
    @Html.ActionLink(genreName, "Browse", new { genre = genreName }, null)

    @if (ViewBag.Starred.Contains(genreName))
    {
        <img src="../../Content/Images/star.png" alt="Starred element" />
    }
</li>
}


